  "version": "2.0.0",
  "tasks": [
    {
      "label": "BuildCompilerDebug",
      "type": "shell",
      "command": "gcc -g lex.yy.c calc3.tab.c compiler.c -o calc3c",
      "group": "build",
    },
    {
      "label": "BuildInterpreterDebug",
      "type": "shell",
      "command": "gcc -g lex.yy.c calc3.tab.c interpreter.c -o calc3i",
      "group": "build",
    },
    {
      "label": "BuildGraphDebug",
      "type": "shell",
      "command": "gcc -g lex.yy.c calc3.tab.c graph.c -o calc3g",
      "group": "build",
    },
    {
      "label": "FlexGenerate",
      "type": "shell",
      "command": "flex calc3.l",
      "group": "build",
    },
    {
      "label": "BisonGenerate",
      "type": "shell",
      "command": "bison -d -b calc3 calc3.y",
      "group": "build"
    }
  ]
